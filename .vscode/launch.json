{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "debug",
			"type": "lldb",
			"request": "custom",
			"initCommands": [
				"platform select remote-windows",
				"platform connect connect://windows-11:12345",
			],
			"targetCreateCommands": [
				"target create ${fileBasenameNoExtension}_a64.exe",
			],
			"processCreateCommands": [
				"process launch",
			],
			"exitCommands": [
				"process kill",
			],
		},
	],
}
